    $ngx_addon_dir/ngx_nlnk_variables.c $ngx_addon_dir/ngx_nlnk_handler.c "

    . auto/module

    ngx_module_type=NGX_NLNK
    ngx_module_name=ngx_nlnk_core_module
    ngx_module_srcs="$ngx_addon_dir/ngx_nlnk_core_module.c"
    
   . auto/module

    ngx_module_type=NGX_NLNK
